{
  "version": 2,
  "builds": [
    {
      "src": "api/**",
      "use": "@vercel/php"
    },
    {
      "src": "index.php",
      "use": "@vercel/php"
    },
    {
      "src": "about.php",
      "use": "@vercel/php"
    },
    {
      "src": "contact-us.php",
      "use": "@vercel/php"
    },
    {
      "src": "projects.php",
      "use": "@vercel/php"
    },
    {
      "src": "security-services.php",
      "use": "@vercel/php"
    },
    {
      "src": "leasing-services.php",
      "use": "@vercel/php"
    },
    {
      "src": "marine-logistics.php",
      "use": "@vercel/php"
    },
    {
      "src": "engineering-services.php",
      "use": "@vercel/php"
    },
    {
      "src": "procurement-consultancy.php",
      "use": "@vercel/php"
    },
    {
      "src": "clearing-forwarding.php",
      "use": "@vercel/php"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/(.*\\.html)$",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ],
  "env": {
    "PHP_MEMORY_LIMIT": "256M"
  }
}
